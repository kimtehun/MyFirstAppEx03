<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ex.myfirstappex03">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.ex.myfirstappex03.BookMarkActivity" />
        <activity android:name="com.ex.myfirstappex03.UselessAvtivity" />
        <activity android:name="com.ex.myfirstappex03.FilelistActivity" />
        <activity
            android:name="com.ex.myfirstappex03.MenuActivity"
            android:theme="@style/Transparent" />
        <activity
            android:name="com.ex.myfirstappex03.SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ex.myfirstappex03.MainActivity" />
    </application>

</manifest>